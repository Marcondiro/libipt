; Test resynchronizing after an error
;
; opt:ptxed --resync-on-error
;

org 0x100000
bits 64

; @pt p0: psb()
; @pt p1: fup(3: %l0)
; @pt p2: mode.exec(64bit)
; @pt p3: psbend()

; @pt p4: tip(3: 0x100100)
l0:  call rax
l1:  hlt

; @pt p5: tnt(nttnnt)
; @pt p6: tnt(nttnnt)
; @pt p7: tip(3: 0x100200)
; @pt p8: tnt(nttnnt)

; @pt p9: tip.pgd(3: %l3)
l2:  jmp rax
l3:  nop


