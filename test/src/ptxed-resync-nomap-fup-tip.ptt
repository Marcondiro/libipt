; Test resynchronizing after an error
;
; opt:ptxed --resync-on-error --event:ip
;

org 0x100000
bits 64

; @pt p0: psb()
; @pt p1: fup(3: %l0)
; @pt p2: mode.exec(64bit)
; @pt p3: psbend()

l0:  jmp 0x100100
l1:  hlt

